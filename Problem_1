def solve(N):
    d=0
    for n in range(1,N):
       div=[]
       
       if(N%n==0):
           div.append(n)
           
       else:
            continue
    
       for d in div:
            d+=d
    print('d=',d)    
    if(d==N):
        print('yes')
    else:
        print("NO")


t=int(input())
for _ in range(t):
    N=int(input())
    out_=solve(N)
    print(out_)
        
